{
    "compilerOptions": {
        "outDir": "build",
        "noImplicitAny": true,
        "module": "es2020",
        "target": "ESNext",
        "lib": ["dom", "dom.iterable", "esnext"],
        "types": ["vite/client", "vite-plugin-svgr/client", "vitest/globals"],
        "jsx": "react",
        "baseUrl": "src",
        "allowJs": true,
        "moduleResolution": "node",
        "declaration": true,
        "allowSyntheticDefaultImports": true,
    },
    "include": [
        "src"
    ]
}